<?xml version="1.0" encoding="UTF-8"?>
<fop version="1.0">
	<base>.</base>
	<source-resolution>72</source-resolution>
	<target-resolution>72</target-resolution>
	<default-page-settings height="11.00in" width="8.50in"/>
	<renderers>
		<renderer mime="application/pdf">
			<filterList>
				<value>flate</value>
			</filterList>

			<fonts>
				<!-- auto-detect fonts -->
				<!--<auto-detect/>-->
				<!-- Nanum Square Setting Start -->
				<font kerning="yes" embed-url="font/NanumSquareR.ttf">
					<font-triplet name="NanumSquareR" style="normal" weight="normal"/>
				</font>

				<font kerning="yes" embed-url="font/NanumSquareB.ttf">
					<font-triplet name="NanumSquareB" style="normal" weight="bold"/>
				</font>
				<font kerning="yes" embed-url="font/NotoSansCJKkr-Regular.otf">
					<font-triplet name="Noto Sans CJK KR Regular" style="normal" weight="normal"/>
				</font>
				<font kerning="yes" embed-url="font/NotoSansCJKkr-Bold.otf">
					<font-triplet name="Noto Sans CJK KR Bold" style="normal" weight="bold"/>
				</font>
				
				<!-- End Nanum Square Setting -->
			</fonts>
		</renderer>
	</renderers>
</fop>
