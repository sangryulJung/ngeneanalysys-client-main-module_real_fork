<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="mainVBox" minWidth="350.0" prefHeight="256.0" style="-fx-background-color: #FFFFFF;" stylesheets="@../../css/variantDetail.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ngeneanalysys.controller.fragment.DetailSubInfoController">
   <children>
      <HBox alignment="CENTER" prefHeight="23.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label styleClass="title1" text="External Links" />
            <ComboBox fx:id="externalLinkComboBox" onAction="#showLinkEvent" prefWidth="120.0" promptText="Please select...">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></ComboBox>
            <Label onMouseClicked="#showIGV" style="-fx-cursor: hand;" styleClass="title1" text="View In IGV" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
