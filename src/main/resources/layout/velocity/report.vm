<?xml version="1.0" encoding="UTF-8"?>
<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format">
	<!-- Layout setting -->
	<fo:layout-master-set>
		<fo:simple-page-master master-name="all-pages" margin-top="5mm" margin-bottom="5mm" margin-left="5mm" margin-right="5mm" page-width="210mm" page-height="297mm">
			#if($isDraft)
			<fo:region-body region-name="xsl-region-body" margin-top="20mm" margin-bottom="25mm" margin-left="10mm" margin-right="10mm" background-position-horizontal="center" background-position-vertical="center" background-repeat="no-repeat" background-image="${draftImageURL}" />
			#else
			<fo:region-body region-name="xsl-region-body" margin-top="20mm" margin-bottom="25mm" margin-left="10mm" margin-right="10mm" />
			#end
			<!--<fo:region-body region-name="xsl-region-body" margin-top="20mm" margin-bottom="25mm" margin-left="10mm" margin-right="10mm" />-->
			<fo:region-before region-name="xsl-region-before" display-align="after" extent="15mm" />
			<fo:region-after region-name="xsl-region-after" display-align="before" extent="25mm" />
		</fo:simple-page-master>
		<fo:page-sequence-master master-name="default-sequence">
			<fo:repeatable-page-master-reference page-position="any" master-reference="all-pages"/>
		</fo:page-sequence-master>
	</fo:layout-master-set>

	<!-- Page Contents -->
	<fo:page-sequence master-reference="all-pages" margin-left="3mm" margin-right="3mm">
		<!-- header contents -->
		<fo:static-content flow-name="xsl-region-before">
			<fo:block font-size="7pt" text-align="left" border-width="0">
				<fo:table table-layout="fixed" width="100%" margin-top="3mm" margin-bottom="3mm" margin-left="3mm" margin-right="3mm">
					<fo:table-column column-width="100mm" />
					<fo:table-column column-width="proportional-column-width(1)" />
					<fo:table-column column-width="80mm" />
					<fo:table-body>
						<fo:table-row>
							<fo:table-cell column-number="1">
								<fo:block text-align="left">
									<!--<fo:external-graphic src="${ngenebioLogo}" content-width="35mm" content-height="6mm"/>-->
								</fo:block>
								<fo:block text-align="left">
									NGenebio $contents.panelName ($contents.panelCode) Clinical Report
								</fo:block>
							</fo:table-cell>
						</fo:table-row>
					</fo:table-body>
				</fo:table>
			</fo:block>
		</fo:static-content>
		<!-- End header contents -->

		<!-- footer contents -->
		<fo:static-content flow-name="xsl-region-after">
			<fo:block margin="5mm" font-size="7pt" text-align="left" border-width="0pt">
				<fo:table table-layout="fixed" width="100%">
					<fo:table-column column-width="80%" />
					<fo:table-column column-width="proportional-column-width(1)" />
					<fo:table-body>
						<fo:table-row>
							<fo:table-cell>
								<fo:block text-align="left">NGeneBio. 1104, Hanhwa Bizmetro, 242, Digital-ro, Guro-gu, Seoul, Korea</fo:block>
								<fo:block text-align="left">Phone: +82-2-867-9787 E-mail: support@ngenebio.com</fo:block>
								<fo:block text-align="left">www.ngenebio.com</fo:block>
							</fo:table-cell>
							<fo:table-cell>
								<fo:block text-align="right"><fo:page-number/> of <fo:page-number-citation ref-id="lastPage"/></fo:block>
							</fo:table-cell>
						</fo:table-row>
					</fo:table-body>
				</fo:table>
			</fo:block>
		</fo:static-content>
		<!-- End footer contents -->

		<!-- Contents Area -->
		<fo:flow flow-name="xsl-region-body">

			<!-- contents page -->
			<fo:block text-align="center" margin-top="10mm" font-family="Noto Sans CJK KR Bold" font-weight="bold" font-size="17pt" margin-bottom="5mm">Clinical Report</fo:block>

			<!-- General Information -->
			<fo:block margin-bottom="10mm" font-size="10pt">
				<fo:block font-size="13pt" font-weight="bold" margin-bottom="1mm">1. General Information</fo:block>
				<fo:block>
					<fo:table table-layout="fixed" width="100%">
						<fo:table-column column-width="proportional-column-width(0.25)"/>
						<fo:table-column column-width="proportional-column-width(0.25)"/>
						<fo:table-column column-width="proportional-column-width(0.25)"/>
						<fo:table-column column-width="proportional-column-width(0.25)"/>
						<fo:table-body>
							<fo:table-row>
								<fo:table-cell display-align="center">
									<fo:block-container height="25mm" border-style="solid" border-width="0.5pt" text-align="center" background-color="#ed7d31">
										<fo:block padding-bottom="20pt" font-weight="bold" color="#FFFFFF">
											Patient Code
										</fo:block>
										<fo:block font-weight="bold" font-size="12pt" color="#FFFFFF">
											SS17-01182
										</fo:block>
									</fo:block-container>
								</fo:table-cell>
								<fo:table-cell display-align="center">
									<fo:block-container height="25mm" border-style="solid" border-width="0.5pt" text-align="center" background-color="#ffc000">
										<fo:block padding-bottom="20pt" font-weight="bold" color="#FFFFFF">
											Sample Source
										</fo:block>
										<fo:block font-weight="bold" font-size="12pt" color="#FFFFFF">
											$contents.sampleSource
										</fo:block>
									</fo:block-container>
								</fo:table-cell>
								<fo:table-cell display-align="center">
									<fo:block-container height="25mm" border-style="solid" border-width="0.5pt" text-align="center" background-color="#70ad47">
										<fo:block padding-bottom="20pt" font-weight="bold" color="#FFFFFF">
											Disease Name
										</fo:block>
										<fo:block font-weight="bold" font-size="12pt" color="#FFFFFF">
											$contents.diseaseName
										</fo:block>
									</fo:block-container>
								</fo:table-cell>
								<fo:table-cell display-align="center">
									<fo:block-container height="25mm" border-style="solid" border-width="0.5pt" text-align="center" background-color="#5b9bd5">
										<fo:block padding-bottom="20pt" font-weight="bold" color="#FFFFFF">
											Panel
										</fo:block>
										<fo:block font-weight="bold" font-size="9pt" color="#FFFFFF">
											$contents.panelName
										</fo:block>
									</fo:block-container>
								</fo:table-cell>
							</fo:table-row>
						</fo:table-body>
					</fo:table>
				</fo:block>

			</fo:block>

			<!-- Variants of Strong/Potential Clinical Significance -->
			<fo:block font-size="10pt" padding-bottom="5mm">
				<fo:block font-size="13pt" font-weight="bold" margin-bottom="1mm">2. Variant(s) with Clinical Significance</fo:block>
				<fo:block>
					<fo:table table-layout="fixed" width="100%" border="solid 0.5mm white">
						<fo:table-column column-width="proportional-column-width(0.2)" />
						<fo:table-column column-width="proportional-column-width(0.2)" />
						<fo:table-column column-width="proportional-column-width(0.2)" />
						<fo:table-column column-width="proportional-column-width(0.2)" />
						<fo:table-column column-width="proportional-column-width(0.2)" />
						<fo:table-body>
							<fo:table-row height="10mm" display-align="center" text-align="center" border="solid 0.5mm white">
								<fo:table-cell border-style="solid" border-width="0.5pt" number-rows-spanned="2" background-color="#e7e7e7" border="solid 0.5mm white">
									<fo:block font-weight="bold">Genomic Alternations</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" number-columns-spanned="2" background-color="#fbe5d6" border="solid 0.5mm white">
									<fo:block font-weight="bold">Tier I</fo:block>
									<fo:block>(Strong Clinical Significance)</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" number-columns-spanned="2" background-color="#fff2cc" border="solid 0.5mm white">
									<fo:block font-weight="bold">Tier II</fo:block>
									<fo:block>(Potential Clinical Significance)</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row height="10mm" font-size="9pt" border="solid 0.5mm white">
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fbe5d6" border="solid 0.5mm white">
									<fo:block font-weight="bold">Level A Evidence</fo:block>
									<fo:block><![CDATA[FDA-approved therapy & Included in professional guidelines]]></fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fbe5d6" border="solid 0.5mm white">
									<fo:block font-weight="bold">Level B Evidence</fo:block>
									<fo:block>Well-powered studies with consensus from experts in the field</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fff2cc" border="solid 0.5mm white">
									<fo:block font-weight="bold">Level C Evidence</fo:block>
									<fo:block><![CDATA[FDA-approved therapies (off label) & Multiple small published studies with some consensus]]></fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fff2cc" border="solid 0.5mm white">
									<fo:block font-weight="bold">Level D Evidence</fo:block>
									<fo:block>Preclinical trials or a few case reports without consensus</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row text-align="center" height="10mm" border="solid 0.5mm white">
								<fo:table-cell border-style="solid" border-width="0.5pt" display-align="center" background-color="#bfbfbf" border="solid 0.5mm white">
									<fo:block font-weight="bold">Total Count</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" display-align="center" background-color="#f8cbad" border="solid 0.5mm white">
									<fo:block>$contents.evidenceACount</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" display-align="center" background-color="#f8cbad" border="solid 0.5mm white">
									<fo:block>$contents.evidenceBCount</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" display-align="center" background-color="#ffe699" border="solid 0.5mm white">
									<fo:block>$contents.evidenceCCount</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" display-align="center" background-color="#ffe699" border="solid 0.5mm white">
									<fo:block>$contents.evidenceDCount</fo:block>
								</fo:table-cell>
							</fo:table-row>
							#foreach($variant in $contents.variantList)
								<fo:table-row text-align="center" border="solid 0.5mm white">
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#e7e7e7" border="solid 0.5mm white">
										<fo:block text-align="left" font-weight="bold">$variant.sequenceInfo.gene
											<fo:leader font-size="5pt" font-weight="normal"/>$variant.variantExpression.variantType
										</fo:block>
										<fo:block text-align="left">$variant.variantExpression.ntChange</fo:block>
									</fo:table-cell>
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fbe5d6" border="solid 0.5mm white">
											<fo:block>$variant.interpretation.interpretationEvidenceA</fo:block>
									</fo:table-cell>
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fbe5d6" border="solid 0.5mm white">
										<fo:block>$variant.interpretation.interpretationEvidenceB</fo:block>
									</fo:table-cell>
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fff2cc" border="solid 0.5mm white">
										<fo:block>$variant.interpretation.interpretationEvidenceC</fo:block>
									</fo:table-cell>
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#fff2cc" border="solid 0.5mm white">
										<fo:block>$variant.interpretation.interpretationEvidenceD</fo:block>
									</fo:table-cell>
								</fo:table-row>
							#end
						</fo:table-body>
					</fo:table>
				</fo:block>
			</fo:block>

			<fo:block font-size="10pt">
				<fo:block font-size="13pt" font-weight="bold" margin-bottom="1mm">3. Negative Result</fo:block>
				<fo:block>
					<fo:table table-layout="fixed" width="100%" border="solid 0.5mm white">
						<fo:table-column column-width="proportional-column-width(0.2)" />
						<fo:table-column column-width="proportional-column-width(0.8)" />
						<fo:table-body>
							<fo:table-row height="7mm" display-align="center">
								<fo:table-cell border-style="solid" border-width="0.5pt" number-columns-spanned="2" background-color="#bfbfbf" border="solid 0.5mm white">
									<fo:block font-weight="bold">
										NON-Small Cell Lung Cancer related or hotspot genes with NO RESULT
									</fo:block>
								</fo:table-cell>
							</fo:table-row>
							#foreach($negative in $contents.negativeList)
								<fo:table-row>
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#e7e7e7" border="solid 0.5mm white">
										<fo:block text-align="left" font-weight="bold">$negative.sequenceInfo.gene</fo:block>
										<fo:block text-align="left">$negative.variantExpression.ntChange</fo:block>
									</fo:table-cell>
									<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#e7e7e7" border="solid 0.5mm white">
										<fo:block>$negative.interpretation.interpretationNegativeTesult</fo:block>
									</fo:table-cell>
								</fo:table-row>
							#end
						</fo:table-body>
					</fo:table>
				</fo:block>
				<fo:block page-break-before="always"> </fo:block>
			</fo:block>


			<!-- Variant Details -->
			<fo:block text-align="center" margin-top="10mm" margin-bottom="5mm" font-family="Noto Sans CJK KR Bold" font-weight="bold" font-size="17pt">Variant Details</fo:block>
			<fo:block margin-bottom="10mm" font-size="10pt">
				<fo:block margin-bottom="5mm">
					#foreach($variant in $contents.variantList)
					#if($variant.swTier)
						#if("$variant.swTier" == "T1")
							#set($titleColor = "#f8cbad")
							#set($color = "#fbe5d6")
						#else
							#set($titleColor = "#ffe699")
							#set($color = "#fff2cc")
						#end
					#end
					<fo:table table-layout="fixed" width="100%" margin-bottom="4mm" border="solid 0.5mm white">
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.25)" />
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.35)" />
						<fo:table-body>
							<fo:table-row height="6mm" display-align="center">
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="$titleColor" border="solid 0.5mm white">
									<fo:block text-align="center">$variant.sequenceInfo.gene</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="3" border-style="solid" border-width="0.5pt" background-color="$titleColor" border="solid 0.5mm white">
									<fo:block text-align="center">$variant.variantExpression.transcript:<![CDATA[$variant.variantExpression.ntChange]]>(<![CDATA[$variant.variantExpression.aaChange]]>)</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$titleColor" border="solid 0.5mm white">
									<fo:block text-align="center">Tier #if($variant.swTier == "T1") I
													#elseif($variant.swTier == "T2") II
													#elseif($variant.swTier == "T3") III
													#end
									</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row border-bottom="1mm" border-top="1mm" display-align="center">
								<fo:table-cell number-columns-spanned="6" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold">
										#if ($variant.interpretation.interpretationEvidenceA && !($variant.interpretation.interpretationEvidenceA == "")) <![CDATA[[LEVEL A]]]> $variant.interpretation.interpretationEvidenceA
										#elseif ($variant.interpretation.interpretationEvidenceB && !($variant.interpretation.interpretationEvidenceB == "")) <![CDATA[[LEVEL B]]]> $variant.interpretation.interpretationEvidenceB
										#elseif ($variant.interpretation.interpretationEvidenceC && !($variant.interpretation.interpretationEvidenceC == "")) <![CDATA[[LEVEL C]]]> $variant.interpretation.interpretationEvidenceC
										#elseif ($variant.interpretation.interpretationEvidenceD && !($variant.interpretation.interpretationEvidenceD == "")) <![CDATA[[LEVEL D]]]> $variant.interpretation.interpretationEvidenceD
										#end
									</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row display-align="center" height="6mm" background-color="$color">
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold">Variant Type
									</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#f2f2f2" border="solid 0.5mm white">
									<fo:block><![CDATA[$variant.variantExpression.variantType ($variant.variantExpression.codingConsequence variant)]]>
									</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold">dbSNP
									</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#f2f2f2" border="solid 0.5mm white">
									<fo:block>$variant.populationFrequency.dbsnpRsId
									</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row display-align="center" height="6mm" background-color="$color">
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold" font-size="9pt">Variant Frequency
									</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#f2f2f2" border="solid 0.5mm white">
									<fo:block>0.509
									</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold">Zygosity
									</fo:block>
								</fo:table-cell>
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#f2f2f2" border="solid 0.5mm white">
									<fo:block>#if($variant.variantExpression.zygosity && !($variant.variantExpression.zygosity == "")) $variant.variantExpression.zygosity
												#else Not-determied
												#end
									</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row>
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold">Population DB (MAF)
									</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="4" border-style="solid" border-width="0.5pt" background-color="#f2f2f2" border="solid 0.5mm white">
									<fo:block>ESP6500: #if($variant.populationFrequency.esp6500) $variant.populationFrequency.esp6500
														#else no frequency data
														#end
									</fo:block>
									<fo:block>1000G: #if($variant.populationFrequency.g1000) $variant.populationFrequency.g1000
										#else no frequency data
										#end
									</fo:block>
									<fo:block>ExAC: #if($variant.populationFrequency.exac) $variant.populationFrequency.exac
										#else no frequency data
										#end
									</fo:block>
								</fo:table-cell>
							</fo:table-row>
							<fo:table-row>
								<fo:table-cell number-columns-spanned="2" border-style="solid" border-width="0.5pt" background-color="$color" border="solid 0.5mm white">
									<fo:block font-weight="bold">Clinical DB (ClnVar)
									</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="4" border-style="solid" border-width="0.5pt" background-color="#f2f2f2" border="solid 0.5mm white">
									<fo:block>
										#if($variant.clinicalSignificant.clinVar.clinVarAcc)
											$variant.clinicalSignificant.clinVar.clinVarAcc($variant.clinicalSignificant.clinVar.clinVarClass)
										#else
											Not found
										#end</fo:block>
								</fo:table-cell>
							</fo:table-row>
						</fo:table-body>
					</fo:table>
					#end
				</fo:block>
				<fo:block page-break-before="always"> </fo:block>
			</fo:block>

			<!-- tierThreeVariantList -->
			<fo:block margin-bottom="10mm" font-size="10pt">
				<fo:block margin-bottom="5mm">
					#foreach($variant in $contents.tierThreeVariantList)
					<fo:table table-layout="fixed" width="100%" margin-bottom="4mm" border="solid 0.5mm white">
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.25)" />
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.1)" />
						<fo:table-column column-width="proportional-column-width(0.35)" />
						<fo:table-body>
							<fo:table-row height="6mm" display-align="center">
								<fo:table-cell border-style="solid" border-width="0.5pt" background-color="#d9d9d9" border="solid 0.5mm white">
									<fo:block text-align="center">$variant.sequenceInfo.gene</fo:block>
								</fo:table-cell>
								<fo:table-cell number-columns-spanned="3" border-style="solid" border-width="0.5pt" background-color="#d9d9d9" border="solid 0.5mm white">
